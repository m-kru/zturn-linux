main bus
  align = 1024 # Align address space to page size.

  # Config for write-read test.
  write_read_test config

  led_red config; width = 1; init-value = 1

  gpio block
    leds config; width = 1; init-value = 1
    switches status; width = 4

  timer block
    counter status
    start proc
      period param
    stop proc

  apb_test block
    masters = 2
    write_read_reg config